#include<iostream>
using namespace std;
class room{
    public:
    double lenth;
    double breadth;
    double height;
double calculatearea()
{
    return lenth*breadth;
}
double calculatevolume ()
{
    return lenth*breadth*height;
}
};
int main()
    {
        room room1;
        room1.lenth=42.5;
        room1.breadth=30.8;
        room1.height=19.2;
        cout<<"area of room="<<room1.calculatearea()<<endl;
        cout<<"volume of room="<<room1.calculatevolume ()<<endl;
        return 0;
    }
